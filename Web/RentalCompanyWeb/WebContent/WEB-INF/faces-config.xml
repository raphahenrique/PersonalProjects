<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>home</managed-bean-name>
		<managed-bean-class>edu.umkc.team6.logic.Home</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>user</managed-bean-name>
		<managed-bean-class>edu.umkc.team6.logic.User</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>searchDataGrid</managed-bean-name>
		<managed-bean-class>edu.umkc.team6.logic.SearchDataGrid</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>end</managed-bean-name>
		<managed-bean-class>edu.umkc.team6.logic.End</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>home.xhtml</display-name>
		<from-view-id>/home.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Search</from-outcome>
			<to-view-id>/search.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>search.xhtml</display-name>
		<from-view-id>/search.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>FinishPage</from-outcome>
			<to-view-id>/finish.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>finish.xhtml</display-name>
		<from-view-id>/finish.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>End</from-outcome>
			<to-view-id>/end.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
